*t30 := t31
t36 := &company 
t37 := #128 * i
t35 := t36 + t37
t38 := t35 + #124
t40 := *t38
t39 := avg + t40
avg := t39
t41 := i + #1
i := t41
GOTO label4
LABEL label6 :
