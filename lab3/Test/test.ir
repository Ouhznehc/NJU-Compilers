FUNCTION calculate_avg :
PARAM g
sum := #0
k := #0
LABEL label1 :
IF k < #10 GOTO label2
GOTO label3
LABEL label2 :
t1 := g + #4
t3 := #12 * k
t2 := t1 + t3
t4 := t2 + #4
t6 := *t4
t5 := sum + t6
sum := t5
t7 := k + #1
k := t7
GOTO label1
LABEL label3 :
t8 := g + #124
t9 := sum / #10
*t8 := t9
t10 := g + #124
RETURN t10
FUNCTION main :
