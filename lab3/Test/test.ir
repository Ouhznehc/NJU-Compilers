i := #0
avg := #0
LABEL label4 :
IF i < #5 GOTO label5
GOTO label6
LABEL label5 :
j := #0
t12 := &company 
t13 := #128 * i
t11 := t12 + t13
t14 := t11 + #0
*t14 := i
LABEL label7 :
IF j < #10 GOTO label8
GOTO label9
LABEL label8 :
t16 := &company 
t17 := #128 * i
t15 := t16 + t17
t18 := t15 + #4
t20 := #12 * j
t19 := t18 + t20
t21 := t19 + #4
t22 := i * #100
t23 := #3000 + t22
t24 := j * #150
t25 := t23 + t24
*t21 := t25
t26 := j + #1
j := t26
GOTO label7
LABEL label9 :
t28 := &company 
t29 := #128 * i
t27 := t28 + t29
t30 := t27 + #124
t33 := &company 
t34 := #128 * i
t32 := t33 + t34
ARG t32
