FUNCTION fact :
PARAM v1
WRITE v1
IF v1 == #1 GOTO label1
GOTO label2
LABEL label1 :
RETURN v1
GOTO label3
LABEL label2 :
t2 := v1 - #1
ARG t2
t1 := CALL fact
t3 := v1 * t1
RETURN t3
LABEL label3 :
FUNCTION main :
READ t4
v2 := t4
IF v2 > #1 GOTO label4
GOTO label5
LABEL label4 :
ARG v2
t5 := CALL fact
v3 := t5
GOTO label6
LABEL label5 :
v3 := #1
LABEL label6 :
WRITE v3
RETURN #0
