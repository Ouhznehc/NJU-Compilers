.data
# For implement convience, store both variable and temp in static area
_prompt: .asciiz "Enter an integer:"
_ret: .asciiz "\n"

# temp var declaration
# variable declaration

.globl main

.text
# Function definitions
read:
   li $v0, 4
   la $a0, _prompt
   syscall
   li $v0, 5
   syscall
   jr $ra

write:
   li $v0, 1
   syscall
   li $v0, 4
   li $a0, _ret
   syscall
   move $v0, $0
   jr $ra

main:
   # WRITE #2
   li $a0, 2
   addi $sp, $sp, -4
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 4
   # WRITE #0
   li $a0, 0
   addi $sp, $sp, -4
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 4
   # WRITE #1
   li $a0, 1
   addi $sp, $sp, -4
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 4
   # WRITE #7
   li $a0, 7
   addi $sp, $sp, -4
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 4
   # RETURN #0
   li $v0, 0
   jr $ra
